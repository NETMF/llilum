    <AndroidAPILevelNumber Condition="'$(AndroidAPILevelNumber)' == ''">$(DefaultAndroidAPILevelNumber)</AndroidAPILevelNumber>
    <TargetPlatformVersion>$(AndroidAPILevelNumber).0</TargetPlatformVersion>

